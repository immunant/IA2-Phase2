RUN: cp /usr/include/libusb-1.0/libusb.h %T/libusb.h
RUN: %ia2_generate_checks /usr/include/libusb-1.0/libusb.h %S/include %T/libusb.h
RUN: ia2-header-rewriter --output-header %T/fn_ptr_ia2.h %T/wrapper.c %T/libusb.h -- -I%resource_dir -I%ia2_include
RUN: cat %T/libusb.h | sed 's/^.*CHECK.*$//' | FileCheck %T/libusb.h
RUN: cat %T/libusb.h | sed 's/^.*CHECK.*$//' | FileCheck --check-prefix 'TYPEDEF-CHECK' %T/libusb.h
